<problem id="eip-id1164335822550">
  <para id="element-642">Given events <m:math>
	<m:mi>J</m:mi>
</m:math> and <m:math>
	<m:mi>K</m:mi>
	<m:mo>:</m:mo>
	<m:mspace width="0.2em"/>
