   </m:mrow>
   <m:mn>2</m:mn>
  </m:msubsup>

 </m:mrow>
</m:math> and <m:math>
 <m:mrow>
  <m:msub>
   <m:mi>H</m:mi>
   <m:mn>0</m:mn>
  </m:msub>
  <m:mo>:</m:mo><m:msubsup>
   <m:mi>σ</m:mi>
   <m:mrow>
    <m:mi>T</m:mi><m:mn>1</m:mn>
