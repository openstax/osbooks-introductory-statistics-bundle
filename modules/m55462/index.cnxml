   <m:mi>t</m:mi>
   <m:mrow>
    <m:mn>0.02</m:mn>
   </m:mrow>
  </m:msub>

 </m:mrow>
</m:math>
 = 2.150</para>
 <para id="eip-idp903520"><m:math>
  <m:mrow>
   <m:mi>E</m:mi><m:mi>B</m:mi><m:mi>M</m:mi><m:mo>=</m:mo><m:msub>
    <m:mi>t</m:mi>
    <m:mrow>
     <m:mfrac>
      <m:mi>α</m:mi>
      <m:mn>2</m:mn>
     </m:mfrac>

    </m:mrow>
   </m:msub>
